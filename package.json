{
    "name": "cursor-diff-apply-pro",
    "displayName": "Cursor Diff Apply Pro",
    "description": "Better diff apply for more optimized code",
    "version": "0.0.1",
    "publisher": "RiddleHe",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": ["Programming Languages"],
    "main": "./out/extension.js",
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:typescript"
    ],
    "contributes": {
        "commands": [
            {
                "command": "diff-apply-pro.analyzeFile",
                "title": "Analyze areas of improvement"
            },
            {
                "command": "diff-apply-pro.applyOptimizations",
                "title": "Apply code optimizations"
            },
            {
                "command": "diff-apply-pro.cancelOptimizations",
                "title": "Cancel optimization session"
            }
        ],
        "keybindings": [
            {
                "command": "diff-apply-pro.analyzeFile",
                "key": "ctrl+shift+o",
                "mac": "cmd+shift+o",
                "when": "editorTextFocus"
            },
            {
                "command": "diff-apply-pro.applyOptimizations",
                "key": "ctrl+shift+a",
                "mac": "cmd+shift+a",
                "when": "editorTextFocus"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "diff-apply-pro.analyzeFile",
                    "group": "navigation",
                    "when": "editorLangId == javascript || editorLangId == typescript"
                }
            ]
        },
        "configuration": {
            "properties": {
                "diff-apply-pro.openRouterApiKey": {
                    "type": "string",
                    "default": "",
                    "description": "OpenRouter API key"
                },
                "diff-apply-pro.morphApiKey": {
                    "type": "string",
                    "default": "",
                    "description": "Morph API key"
                }
            }
        }
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.74.0",
        "@types/node": "^16.x",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "axios": "^1.6.0"
    }
}